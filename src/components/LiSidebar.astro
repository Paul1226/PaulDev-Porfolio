---
interface Props {
    titulo: string
    url: string
}
const { titulo, url } = Astro.props
---

<li class="">
    <a href={url} class="flex flex-col justify-center items-center w-full [aspect-ratio:1/1] bg-chill-400/50 rounded-lg gap-1 p-1">
        <slot name="icon" />
        <span class="leading-[10px] capitalize text-xs">{titulo}</span>
    </a>
</li>